{
  "name": "glamora-fullstack-app",
  "version": "1.0.0",
  "description": "Glamora Fashion App - Full Stack Application with React Native Frontend and Node.js Backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon server.js",
    "dev-backend": "cd ../backend && nodemon server.js",
    "dev-admin": "cd ../admin-side && npx http-server . -p 8080 -o",
    "dev-expo": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "echo 'Full stack build complete'",
    "test": "echo 'No tests specified'",
    "lint": "expo lint",
    "reset-project": "node ./scripts/reset-project.js",
    "install-deps": "cd ../backend && npm install",
    "admin-start": "npx http-server ../admin-side -p 8080 -o",
    "admin-login": "npx http-server ../admin-side -p 8080 -o login.html",
    "admin-analytics": "npx http-server ../admin-side -p 8080 -o analytics.html",
    "admin-users": "npx http-server ../admin-side -p 8080 -o user-management.html",
    "admin-moderation": "npx http-server ../admin-side -p 8080 -o content-moderation.html"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "socket.io": "^4.8.1",
    "nodemailer": "^7.0.5",
    "mongodb": "^6.17.0",
    "firebase": "^11.10.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "expo": "53.0.20",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-image-picker": "^16.1.4",
